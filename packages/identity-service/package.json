{
  "name": "@infinityvault/identity-service",
  "version": "1.0.0",
  "description": "User identity bridging service for InfinityVault multi-system architecture",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.24",
    "@vitest/coverage-v8": "^1.3.1",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "^5.6.3",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "express": "^4.18.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  },
  "keywords": [
    "identity",
    "authentication",
    "jwt",
    "user-management",
    "multi-system"
  ],
  "author": "InfinityVault",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinityvault/common-libs.git",
    "directory": "packages/identity-service"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}